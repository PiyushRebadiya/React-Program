\n    const DeleteCountry = (e, id) => {\r\n        setBlank(blank.filter((item) => item.Id !== id))\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {blank && blank.map((item) => {\r\n                return (\r\n                    <div>\r\n                        <textarea\r\n                            type='text'\r\n                            name='Address'\r\n                            onChange={(e) => { countryItem(e, item.Id) }}\r\n                            placeholder='Add a Text'\r\n                        /><br />\r\n                        <select\r\n                            name='Country'\r\n                            onChange={(e) => { countryItem(e, item.Id) }}>\r\n                            <option\r\n                                disabled selected=\"selected\">\r\n                                ---Selected Counrty---\r\n                            </option>\r\n                            <option\r\n                                value='India'>India\r\n                            </option>\r\n                            <option\r\n                                value='US'>US\r\n                            </option>\r\n                            <option\r\n                                value='Europe'>Europe\r\n                            </option>\r\n                        </select><br />\r\n                        <select\r\n                            name='State'\r\n                            onChange={(e) => { countryItem(e, item.Id) }}>\r\n                            <option\r\n                                disabled selected=\"selected\">\r\n                                ---Selected State---\r\n                            </option>\r\n                            <option\r\n                                value='Gujrat'>\r\n                                Gujrat</option>\r\n                            <option\r\n                                value='London'>\r\n                                London</option>\r\n    